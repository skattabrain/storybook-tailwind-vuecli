{
  "name": "vue-storybook-tailwind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run storybook:build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "tailwind:build": "npx tailwindcss build ./src/styles/index.css -o ./src/styles.css",
    "tailwind:build:purge": "npx tailwindcss build ./src/styles/index.css -o ./src/styles.css -purge",
    "chromatic": "npx chromatic --build-script-name=storybook:build"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "bootstrap-icons": "^1.1.0",
    "core-js": "^3.7.0",
    "focus-visible": "^5.2.0",
    "lodash": "^4.17.20",
    "postcss": "^7.0.35",
    "tailwindcss": "^2.0.1-compat",
    "typeface-lato": "^0.0.75",
    "typeface-noto-sans": "^0.0.72",
    "typeface-nunito-sans": "^0.0.72",
    "typeface-open-sans": "^0.0.75",
    "typeface-quattrocento": "^0.0.72",
    "typeface-roboto": "^0.0.75",
    "typeface-source-sans-pro": "^1.1.13",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vue-screen-size": "^1.0.1",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/client-api": "^6.1.10",
    "@storybook/client-logger": "^6.1.10",
    "@storybook/vue": "^6.1.10",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-plugin-router": "^4.5.9",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/cli-plugin-vuex": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/test-utils": "1.0.3",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "chromatic": "^5.4.0",
    "eslint": "^7.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^6.2.2",
    "postcss-focus-visible": "^5.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.4",
    "postcss-nested": "^4.2.3",
    "stylelint-config-recommended": "^3.0.0",
    "vue-cli-plugin-storybook": "~2.0.0",
    "vue-docgen-loader": "^1.5.0",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.12"
  }
}
